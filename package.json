{
  "name": "graph",
  "version": "1.0.0",
  "description": "example",
  "main": "App.js",
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "watch-backend-api": "nodemon App.js",
    "watch-dialog-constructor": "cd static/dialogConstructorDev && npm run start",
    "sass": "sass",
    "build-style": "sass static/scss/main.scss:static/style/bundle.css",
    "build": "npm run build-style && cd static/dialogConstructorDev && npm run build",
    "postinstall": "cd static/dialogConstructorDev && npm install",
    "watch-style": "sass --watch static/scss/main.scss:static/style/bundle.css",
    "predev": "kill-port 62544 34567",
    "dev": "concurrently npm:watch-*",
    "fix": "prettier . --write --ignore-unknown",
    "start": "nodemon App.js",
    "prepare": "husky install"
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "random-sentence": "^1.0.4",
    "sass": "^1.50.1"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "2.6.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md,html,scss}": "prettier --write"
  }
}
